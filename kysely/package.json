{
  "name": "kysely-playground",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.tests.json --noEmit",
    "migrate:up": "kysely migrate up",
    "migrate:down": "kysely migrate:down",
    "migrate:to": "kysely migrate:to",
    "migrate:reset": "kysely migrate:reset",
    "migrate:new": "kysely create",
    "db:types": "kysely-codegen",
    "db:sync": "npm run migrate:up && npm run db:types",
    "seed": "tsx scripts/seed.ts",
    "lint": "eslint . --fix --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "fastify": "^5.6.1",
    "fastify-type-provider-zod": "^6.0.0",
    "kysely": "^0.28.7",
    "pg": "^8.16.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "1.51.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "kysely-codegen": "^0.19.0",
    "kysely-ctl": "^0.19.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^2.1.3"
  }
}
